<% require "csv" %>
<%= CSV.generate(:col_sep => ";") do |csv|
  csv << ["First Name", "Last Name", "Dob", "Class", "Target Face"]
  @participants.each do |p|
    csv << [
      p.first_name,
      p.last_name,
      p.dob,
      p.tournament_class.name,
      p.target_face.name,
    ]
  end
end %>
